{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "auth": {
    "rolesSource": "/api/.auth/me",
    "identityProviders": {
      "customOpenIdConnectProviders": {}
    }
  }
}
